{
  "name": "cgi-242-server",
  "version": "1.0.0",
  "private": true,
  "description": "Backend Express pour CGI 242 - Multi-Tenant Architecture",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:rag": "tsx src/tests/run-all-tests.ts",
    "test:rag:quick": "tsx src/tests/run-quick-tests.ts",
    "test:rag:green": "tsx src/tests/run-all-tests.ts --level=green",
    "test:rag:yellow": "tsx src/tests/run-all-tests.ts --level=yellow",
    "test:rag:all": "tsx src/tests/run-all-chapters-tests.ts",
    "test:rag:2025": "tsx src/tests/run-all-chapters-tests.ts --2025",
    "test:rag:2026": "tsx src/tests/run-all-chapters-tests.ts --2026",
    "test:rag:il": "tsx src/tests/run-il-tests.ts",
    "test:rag:il:quick": "tsx src/tests/run-il-tests.ts --quick",
    "test:agent": "tsx src/tests/run-agent-tests.ts",
    "test:manual": "tsx src/tests/generate-manual-check.ts",
    "lint": "eslint src --ext .ts --ignore-pattern 'src/archive/**' --ignore-pattern 'src/tests/**' --ignore-pattern 'src/scripts/**'",
    "ingest": "tsx src/scripts/ingest-articles.ts",
    "test:smoke": "tsx src/tests/smoke/smoke-tests.ts",
    "test:performance": "k6 run src/tests/performance/load-test.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@pinecone-database/pinecone": "^2.0.0",
    "@prisma/client": "^5.10.0",
    "@prisma/extension-read-replicas": "^0.3.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "@sentry/node": "^10.32.1",
    "@types/ioredis": "^4.28.10",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "date-fns": "^3.3.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "openai": "^6.15.0",
    "otpauth": "^9.4.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.3.1",
    "stripe": "^20.1.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.10.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
